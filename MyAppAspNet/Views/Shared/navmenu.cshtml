@using System.Dynamic

@model IEnumerable<MyAppAspNet.Models.MenuApp>

@foreach (var item in Model)
{
    var m = Model.Where(a => a.IDParentMenu == item.ID).ToString();
    dynamic menumodel = new ExpandoObject();
    menumodel.menus = Model;
    menumodel.itemmenu = m;
    @Html.Partial("~/Views/Shared/itemmenu.cshtml", menumodel);
}
